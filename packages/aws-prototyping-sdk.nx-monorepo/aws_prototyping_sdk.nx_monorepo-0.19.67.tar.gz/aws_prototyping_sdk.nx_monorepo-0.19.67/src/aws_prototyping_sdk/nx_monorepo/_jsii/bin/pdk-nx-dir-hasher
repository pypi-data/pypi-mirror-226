#!/usr/bin/env python

import jsii
import sys

__jsii_assembly__ = jsii.JSIIAssembly.load(
    "@aws-prototyping-sdk/nx-monorepo",
    "0.19.67",
    "aws_prototyping_sdk.nx_monorepo",
    "nx-monorepo@0.19.67.jsii.tgz",
)

exit_code = __jsii_assembly__.invokeBinScript(
    "@aws-prototyping-sdk/nx-monorepo", "pdk-nx-dir-hasher", sys.argv[1:]
)
exit(exit_code)
