{
    "scope":{
      "source":"sex-age-comorbidity",
      "source_description":"TESTING dataset provided for the deveolopment of PANDEM-2 delivarable on month 12.",
      "tags":["COVID19 Template"],
      "data_quality":"Draft",
      "frequency":"every 30 seconds",
      "reference_user":"Please fill when used",
      "reporting_email":"Please fill when used",
      "globals":[
        {"variable":"source"},
        {"variable":"pathogen_name", "value":"COVID-19"}
      ],
      "update_scope":[
        {"variable":"source", "value":["sex-age-comorbidity"]},
        {"variable":"reporting_date", "value": []}
      ]
    },
    "acquisition":{
      "channel":{
        "name":"input-local",
        "xls_file":"M.12 Upload templates_end_users.xlsx",
        "sheet":"Sex Age Comorbidity"
      },
      "format": {
        "name":"xls",
        "decimal_sign":".",
        "thousands_separator":"",
        "date_format":"%Y-%m-%d",
        "encoding":"UTF-8"
      }
    },
    "columns":[
      {"name":"Date", "variable":"reporting_date"}, 
      {"name":"Country", "variable":"country_name"}, 
      {"name":"NUTS2/3", "variable":"geo_code"},
      {"name":"Comorbidity group", "variable":"comorbidity_code"},
      {"name":"Age group", "variable":"age_group"},
      {"name":"Gender", "variable":"gender_code"},
      {"name":"Population", "variable":"population"},
      {"name":"Confirmed cases", "variable":"confirmed_cases"},
      {"name":"Hospitals admissions", "variable":"number_of_hospitalised_patients"},
      {"name":"ICU admissions", "variable":"icu_patients"},
      {"name":"deaths", "variable":"number_of_deaths"}
    ]
}
