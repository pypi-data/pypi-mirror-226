{
    "scope":{
      "source":"participatory-sentinel-surveilla",
      "source_description":"TESTING dataset provided for the deveolopment of PANDEM-2 delivarable on month 12.",
      "tags":["COVID19 Template"],
      "data_quality":"Draft",
      "frequency":"every 30 seconds",
      "reference_user":"Please fill when used",
      "reporting_email":"Please fill when used",
      "globals":[
        {"variable":"source"},
        {"variable":"pathogen_name", "value":"COVID-19"}
      ],
      "update_scope":[
        {"variable":"source"},
        {"variable":"reporting_date"}
      ]
    },
    "acquisition":{
      "channel":{
        "name":"input-local",
        "xls_file":"M.12 Upload templates_end_users.xlsx",
        "sheet":"ParticipatorySentinel Surveilla"
      },
      "format": {
        "name":"xls",
        "decimal_sign":".",
        "thousands_separator":"",
        "date_format":"%Y-%m-%d",
        "encoding":"UTF-8"
      }
    },
    "columns":[
      {"name":"Date", "variable":"reporting_date"}, 
      {"name":"Country", "variable":"country_name"}, 
      {"name":"Gender", "variable":"gender_code"},
      {"name":"NUTS2/3", "variable":"geo_code"},
      {"name":"Age group", "variable":"age_group"},
      {"name":"Total participants", "variable":"number_of_participants"},
      {"name":"Participants declaring symptoms", "variable":"participants_declaring_symptoms"},
      {"name":"ARI - ILI patients", "variable":"ari_ili_patients"},
      {"name":"postivity for Influenza / ARI - ILI", "variable":"positivity_for_influenza_ari_ili"},
      {"name":"postivity for covid / ARI - ILI", "variable":"posititivity_for_covid_ari_ili"},
      {"name":"Hospitalised SARI patients", "variable":"hospitalised_sari_patients"},
      {"name":"COVID confirmed cases in hospitalised SARI patients", "variable":"hospitalised_sari_infected_patients"}
    ]
}
